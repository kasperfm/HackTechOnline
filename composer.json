{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.3.0",
        "backpack/crud": "4.1.*",
        "backpack/logmanager": "^4.0.0",
        "backpack/permissionmanager": "^6.0",
        "consoletvs/charts": "6.*",
        "facade/ignition": "^2.3.6",
        "guzzlehttp/guzzle": "^7.0.1",
        "guzzlehttp/psr7": "^1.5",
        "highideas/laravel-users-online": "^3.0",
        "intervention/image": "^2.3",
        "jrean/laravel-user-verification": "^v9.0.0",
        "laracasts/utilities": "^3.0",
        "laravel/framework": "^v8.0",
        "laravel/sail": "^1.6",
        "laravel/socialite": "^v5.1.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.2",
        "nunomaduro/collision": "^5.0",
        "php-http/curl-client": "^1.7",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^6.1",
        "sentry/sentry": "^2.5.1",
        "sentry/sentry-laravel": "^1.9.0",
        "spatie/laravel-activitylog": "^3.1"
    },
    "require-dev": {
        "backpack/generators": "^3.0",
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.6",
        "doctrine/dbal": "^2.9",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "~1.4",
        "laracasts/generators": "^1.0",
        "mockery/mockery": "1.*",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Classes/Helpers/GlobalHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/Classes/Helpers/GlobalHelper.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }

}
